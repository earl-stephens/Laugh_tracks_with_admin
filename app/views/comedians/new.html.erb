<div class="new_form_page">
  <img src="http://mediad.publicbroadcasting.net/p/kmuw/files/styles/x_large/public/201703/stand-up-comedy.jpg" alt="microphone on stage">
  <h1>Add a new comedian</h1>

  <section class="new_form">
    <%= form_for @comedian do |f| %>

    <p class="form_fors"><%= f.label :name %>
    <%= f.text_field :name %></p><br/>

    <p class="form_fors"><%= f.label :age %>
    <%= f.number_field :age %></p><br/>

    <p class="form_fors"><%= f.label :city %>
    <%= f.text_field :city %></p><br/>

    <p class="form_fors"><%= f.submit "Add new comedian" %></p>

    <% end %>
  </section>
</div>
